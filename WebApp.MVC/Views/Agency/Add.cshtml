@model FieldAgent.Core.Entities.Agency

<h1>Add new Agency</h1>
<div class="row">
    <div class="col-sm-12 col-md-6 col-lg-4">
        <form asp-controller="Agency" asp-action="Add" method="post">
            <div class="mb-3">
                <label asp-for="ShortName" class="form-label">ShortName</label>
                <input type="text" class="form-control" id="ShortName" asp-for="ShortName" />
            </div>
            <div class="mb-3">
                <label asp-for="LongName" class="form-label">LongName</label>
                <input type="text" class="form-control" id="LongName" asp-for="LongName" />
            </div>
            <button type="submit" class="btn btn-primary">Touch</button>
        </form>
    </div>
</div>
